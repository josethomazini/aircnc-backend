{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "ignore": [
    "node_modules",
    "**/*.spec.ts"
  ],
  "comments": false,
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "@src": "./src/",
          "@configs": "./src/configs",
          "@models": "./src/models",
          "@controllers": "./src/controllers"
        }
      }
    ]
  ]
}
